{
    "name": "semicolonremover",
    "publisher": "bprzemek80",
    "icon": "images/icon.png",
    "displayName": "SemicolonRemover",
    "description": "Will remove last character in every line, that match to the pattern after 'OrganizeImports' command have been launched. ",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Semicolon",
        "Organize Imports"
    ],
    "activationEvents": [
        "onCommand:extension.semicolonRemover"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BeastCode/VSCode-Angular-TypeScript-Snippets.git"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.semicolonRemover",
            "title": "SemicolonRemover"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}